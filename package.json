{
  "name": "lola-communities",
  "description": "A Lightweight website + CMS for Lola Tech Communities",
  "version": "1.0.0",
  "main": "pages/index.js",
  "keywords": [
    "lola",
    "communities",
    "react",
    "nextjs",
    "netlifycms"
  ],
  "license": "UNLICENSED",
  "author": "Nuta Alexandru <alexandru.nuta@lola.tech>",
  "private": true,
  "homepage": "",
  "proxy": "http://localhost:8080",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lolaent/lola-communities.git"
  },
  "lint-staged": {
    "*.{js,json,md,yml}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e",
      "pre-commit": "lint-staged"
    }
  },
  "scripts": {
    "dev": "concurrently \"netlify-cms-proxy-server\" \"next -p 8080\"",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write '{*/**,.}/*.{js,json,yml}'",
    "lint": "eslint ."
  },
  "resolutions": {
    "styled-components": "5.2.1"
  },
  "dependencies": {
    "babel-plugin-styled-components": "1.12.0",
    "gray-matter": "4.0.2",
    "netlify-cms-oauth-provider-node": "1.1.0",
    "next": "11.1.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-slick": "0.27.13",
    "remark": "13.0.0",
    "remark-html": "13.0.1",
    "slick-carousel": "1.8.1",
    "styled-components": "5.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "babel-eslint": "10.1.0",
    "concurrently": "5.3.0",
    "eslint": "7.32.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-flowtype": "5.9.0",
    "eslint-plugin-import": "2.24.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "4.3.8",
    "lint-staged": "10.5.4",
    "netlify-cms-proxy-server": "1.3.19",
    "prettier": "2.3.2",
    "prop-types": "15.7.2"
  }
}

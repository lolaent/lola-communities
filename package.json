{
  "name": "lola-communities",
  "description": "A Lightweight website + CMS for Lola Tech Communities",
  "version": "1.0.0",
  "main": "pages/index.js",
  "keywords": [
    "lola",
    "communities",
    "react",
    "nextjs",
    "netlifycms"
  ],
  "license": "UNLICENSED",
  "author": "Nuta Alexandru <alexandru.nuta@lola.tech>",
  "private": true,
  "homepage": "",
  "proxy": "http://localhost:8080",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lolaent/lola-communities.git"
  },
  "lint-staged": {
    "*.{js,json,md,yml}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e",
      "pre-commit": "lint-staged"
    }
  },
  "scripts": {
    "dev": "concurrently \"netlify-cms-proxy-server\" \"next -p 8080\"",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write '{*/**,.}/*.{js,json,yml}'",
    "lint": "eslint .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "resolutions": {
    "styled-components": "5.1.1"
  },
  "dependencies": {
    "babel-plugin-styled-components": "1.11.1",
    "frontmatter-markdown-loader": "3.6.0",
    "netlify-cms-oauth-provider-node": "1.1.0",
    "next": "9.5.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "styled-components": "5.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "9.1.2",
    "@commitlint/config-conventional": "9.1.2",
    "babel-eslint": "10.1.0",
    "concurrently": "5.3.0",
    "eslint": "7.8.1",
    "eslint-config-react-app": "5.2.1",
    "eslint-plugin-flowtype": "4.7.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.20.3",
    "eslint-plugin-react-hooks": "4.1.0",
    "frontmatter-markdown-loader": "3.6.0",
    "husky": "4.2.5",
    "lint-staged": "10.2.11",
    "netlify-cms-proxy-server": "1.3.2",
    "prettier": "2.1.1",
    "prop-types": "15.7.2"
  }
}
